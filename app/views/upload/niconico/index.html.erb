<div class="subpage">
  <%= render_flash_messages %>
  <%= content_tag :div, class: 'button-field' do %>
    <%= button_tag t('upload.niconico.new_track'), class: 'button', disabled: 'disabled' %>
  <% end %>
  <% if @tracks.length > 0 %>
    <%= render_niconico_track_list(@tracks) %>
  <% else %>
    <p>尚無任何歌曲上傳</p>
  <% end %>
</div>
<%= render_debug_information %>